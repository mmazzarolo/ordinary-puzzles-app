{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.rulers": [100],
  "editor.quickSuggestions": {
    "strings": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#1a0644",
    "activityBar.background": "#1a0644",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#92380d",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#1a0644",
    "statusBar.background": "#080215",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#1a0644",
    "statusBarItem.remoteBackground": "#080215",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#080215",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#08021599",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#080215"
}
